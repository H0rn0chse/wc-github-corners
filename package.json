{
  "name": "wc-github-corners",
  "version": "0.1.4",
  "type": "module",
  "main": "dist/index.es.js",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "github",
    "corners",
    "webcomponents",
    "lit"
  ],
  "scripts": {
    "analyze": "custom-elements-manifest analyze --globs 'src/index.ts' --outdir dist",
    "dev": "vite",
    "build": "tsc && vite build && npm run copy:demo && npm run docs:gen",
    "copy:demo": "cp demo/* dist/",
    "preview": "vite preview",
    "docs:gen": "npm run analyze && esno scripts/gen-docs.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lit": "^2.0.2"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@custom-elements-manifest/to-markdown": "^0.1.0",
    "@types/node": "^17.0.5",
    "@yunyoujun/utils": "^0.1.4",
    "esno": "^0.13.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.9"
  },
  "customElements": "dist/custom-elements.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/wc-github-corners"
  }
}
